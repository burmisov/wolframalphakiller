  "main": "index.js",
  "scripts": {
    "build": "rimraf build/ && babel -d build src/server",
    "start": "npm run build && cross-env NODE_ENV=prod node build/index.js",
    "dev": "nodemon --watch src/ --exec \"npm run lint && cross-env NODE_ENV=dev babel-node src/server/index.js --out-file dist/index.js -- dist/index.js\"",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/",
    "flow": "flow",
    "test": "npm run compile && cross-env NODE_ENV=test mocha --exit .compiled/test/*.js",
    "compile": "babel src --out-dir .compiled --source-maps"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "express": "^4.16.3",
    "express-json-validator-middleware": "^1.2.3",
    "express-winston": "^3.0.0",
    "pg": "^7.4.3",
    "winston": "^3.1.0",
    "winston-transport": "^4.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "chai-json-schema-ajv": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.81.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "prettier": "1.14.3",
    "rimraf": "^2.6.2"
  }
}
